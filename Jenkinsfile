pipeline {
    agent any

    stages {
        stage('Clone Repository') {
            steps {
                echo 'Cloning GitHub Repository...'
                
            }
        }

        stage('Build') {
            steps {
                echo 'Compiling Java code...'
                
            }
        }

        stage('Test') {
            steps {
                echo 'Running Java program...'
                
            }
        }
    }
}
